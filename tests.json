{
  "defaults": {
    "description": "minimap2 workflow test",
    "metrics_calculate": "calculate.sh",
    "metrics_compare": "compare.sh",
    "output_metrics_dir": "/.mounts/labs/gsi/testdata/minimap2/output_metrics",
    "parameters": {
      "wdl_options": {
        "write_to_cache": false,
        "read_from_cache": false
      }
    }
  },
  "tests": [
    {
      "id": "minimap2Test",
      "parameters": {
        "wdl_inputs": {
          "minimap2.ref": "$HG19_ROOT/hg19_random.fa",
          "minimap2.fastqFile" : "/.mounts/labs/gsi/testdata/minimap2/input_data/mergedFastqFile.fastq",
          "minimap2.convert2Sam.modules": "minimap2/2.17 hg19/p13",
          "minimap2.outputFileNamePrefix": "ID_HERE"
        }
      }
    },
    {
      "id": "minimap2EmptyOutputFileNamePrefixTest",
      "parameters": {
        "wdl_inputs": {
          "minimap2.ref": "$HG19_ROOT/hg19_random.fa",
          "minimap2.fastqFile" : "/.mounts/labs/gsi/testdata/minimap2/input_data/mergedFastqFile.fastq",
          "minimap2.convert2Sam.modules": "minimap2/2.17 hg19/p13",
          "minimap2.outputFileNamePrefix": ""
        }
      }
    }
  ]
}
